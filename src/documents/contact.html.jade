---
title: "Contact"
layout: "default"
TopMenuCategory: "Contact"
scripts: ['https://maps.googleapis.com/maps/api/js']
priority: 1.0
---

block content
  section.contact-title.text-center
    .contact-wrapper
      .container
        .row
          div
            h1.headline Contact us
        .row
          .sub-headline
            p 
  section.contact-content
    .container
      .row
        .col-md-8
          .google-map
            #map-canvas
          .quick-contact
            h3 Main Office Contact Details
            hr
            .row
              .col-md-6
                .row
                  .col-md-2
                    p
                      i.fa.fa-home
                  .col-md-10
                    p
                      | Full Address:<br/>
                      | 2nd Floor<br/> 
                      | The Parkside Suite<br/>
                      | Frimley Park Hospital<br/>
                      | Portsmouth Road<br/>
                      | Camberley<br/>
                      | GU16 7UJ
              .col-md-6
                p
                  i.fa.fa-phone
                  span Telephone: <a href="tel:0800 020 9511">0800 020 9511</a>
                p
                  i.fa.fa-fax
                  span Fax: 01276 526868

        .col-md-4
          .appointment
            .header.text-center
              h2 Make an Appointment
              a.number(href='#')
                i.fa.fa-phone.fa-fw
                | 01276 522634
              span.or OR
            // form of appointment
            .row
              form(method='post', action='#')
                .form
                  .input-group.margin-bottom-sm.col-md-8.col-md-offset-2.col-sm-8.col-sm-offset-2.col-xs-10.col-xs-offset-1
                    input.form-control(type='text', placeholder='Full Name *', required='')
                  .input-group.margin-bottom-sm.col-md-8.col-md-offset-2.col-sm-8.col-sm-offset-2.col-xs-10.col-xs-offset-1
                    input.form-control(type='text', placeholder='Email Address *', required='')
                  .input-group.margin-bottom-sm.col-md-8.col-md-offset-2.col-sm-8.col-sm-offset-2.col-xs-10.col-xs-offset-1
                    input.form-control(type='text', placeholder='Appointment Date *', required='')
                  .input-group.margin-bottom-sm.col-md-8.col-md-offset-2.col-sm-8.col-sm-offset-2.col-xs-10.col-xs-offset-1
                    input.form-control(type='text', placeholder='Mobile Number *', required='')
                .input-group.margin-bottom-sm.col-md-8.col-md-offset-2.col-sm-8.col-sm-offset-2.col-xs-10.col-xs-offset-1
                  textarea.form-control(rows='6', placeholder='Your Problem *', required='')
                .col-md-8.col-md-offset-2.col-sm-8.col-sm-offset-2.col-xs-10.col-xs-offset-1
                  .captcha-container
                    label Are you human? 
                    br
                    img(src='http://inspirythemesdemo.com/healthpress/wp-content/themes/healthpress-theme/captcha/captcha.php', alt='')
                    input.captcha.required(type='text', name='captcha', maxlength='5', title=' Please enter the code characters displayed in image!')
                  input.btn.btn-primary.send(type='submit', value='Send')
            // end of form
          // end of appointment

  script.
    function initialize() {
      var mapCanvas = document.getElementById('map-canvas');
        var mapOptions = {
        center: new google.maps.LatLng(24.900392, 91.853181),
        zoom: 16,
        mapTypeId: google.maps.MapTypeId.ROADMAP
      }
      var map = new google.maps.Map(mapCanvas, mapOptions)
    };

    $(document).ready(function() {
      new WOW().init();
      google.maps.event.addDomListener(window, 'load', initialize);
    };